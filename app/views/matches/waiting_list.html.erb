<ul>
  <% @matches.each do |match| %>
    <li><div class="match">
      <span class="player_names">
        <%= render "match_names", :match => match %>
      </span>
      <%= link_to "Start match", matches_start_path(match), {:class => 'button_link', :method => :start} %>
      <%= link_to "Delete match", match_path(match), {:class => 'button_link', :method => :delete} %>
    </div></li>
  <% end %>
</ul>

<%= link_to "Create match", new_match_path, { :class => 'button_link' } %>
